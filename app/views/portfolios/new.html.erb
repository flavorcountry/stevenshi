<%= form_for @portfolio do |f| %>
<%= f.label :name %>
<br>
<%= f.text_field :name %>
<br>

<%= f.label :description %>
<br>
<%= f.text_area :description, rows: 5 %>
<br>

<%= f.label :url %>
<br>
<%= f.text_field :url %>
<br>

<%= f.label :password %>
<br>
<%= f.password_field :password %>
<Br>

<%= f.submit %>

<% end %>